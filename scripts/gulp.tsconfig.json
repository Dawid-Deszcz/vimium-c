{
  "compilerOptions": {
    "listEmittedFiles": true,
    "listFiles": false,
    "outDir": ".",
    "skipLibCheck": true,
    "target": "es5",
    "typeRoots": ["typings"],
    "typescript": "typescript/lib/typescript"
  },
  "build": { // for [ local, dist ]
    "MinCVer": [ 89, 47 ], // keep dist to be 47 : the release version
    "MinFFVer": [ 78, 63 ],
    "BTypes": [ 3, 1 ], // [ Chrome | Firefox, Chrome ]
    "OS": [ 7, 7 ],
    "DetectAPIOnFirefox": [1, 0],
    "NDEBUG": [ 0, 1 ],
    "Mangle": [ 0, 1 ],
    "LessPorts": [ 1, 0 ],
    "Inline": [ 0, 1 ]
  },
  "extends": "../tsconfig.base.json"
}
